# Google Places API
GOOGLE_PLACES_API_KEY=${GOOGLE_PLACES_API_KEY}

# AWS Configuration
AWS_REGION=eu-west-2

# S3 Image Storage
S3_IMAGES_BUCKET=trustdiner-data-334993922596-eu-west-2

# Staging Settings
NODE_ENV=staging
PORT=3001

# Security
CORS_ORIGIN=*
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=1000
